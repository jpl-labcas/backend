# Copy this file to .env and adjust values for your environment.

LABCAS_ENV=development
LABCAS_HOST=0.0.0.0
LABCAS_PORT=8000
LABCAS_LOG_LEVEL=INFO

LABCAS_DIRECTORY_PROVIDER=ldap
LABCAS_LDAP_URI=ldaps://edrn-ds.jpl.nasa.gov
LABCAS_LDAP_BIND_DN=uid=service,dc=edrn,dc=jpl,dc=nasa,dc=gov
LABCAS_LDAP_PASSWORD=changeme
LABCAS_LDAP_USER_BASE=dc=edrn,dc=jpl,dc=nasa,dc=gov
LABCAS_LDAP_GROUP_BASE=dc=edrn,dc=jpl,dc=nasa,dc=gov

LABCAS_JWT_SECRET=super-secret
LABCAS_JWT_ISSUER=labcas
LABCAS_JWT_AUDIENCE=labcas-clients
LABCAS_SESSION_TTL_SECONDS=28800
LABCAS_REDIS_URL=redis://redis:6379/0

LABCAS_SEARCH_ENGINE=solr
LABCAS_SOLR_URL=https://localhost:8984/solr
LABCAS_SOLR_MAX_ROWS=5000
LABCAS_SOLR_VERIFY_SSL=False

AWS_REGION=us-west-2
AWS_ACCESS_KEY_ID=changeme
AWS_SECRET_ACCESS_KEY=changeme
S3_BUCKET=changeme
AWS_DOWNLOAD_URL_EXPIRATION_TIME_SECS=20

# Path prefix replacements for local file debugging (optional)
# Format: "old_prefix:new_prefix" (multiple replacements comma-separated)
# Example: "/usr/local/labcas/backend:/Users/kelly,/labcas-data:/Users/kelly"
# LABCAS_FILE_PATH_PREFIX_REPLACEMENTS=/usr/local/labcas/backend:/Users/kelly,/labcas-data:/Users/kelly

LABCAS_ZIPPERLAB_URL=https://edrn-docker/ops/zipperlab

ACCEPT_ANY_JWT=False
